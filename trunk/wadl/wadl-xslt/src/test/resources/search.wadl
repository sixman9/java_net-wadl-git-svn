<?xml version="1.0"?>
<application xmlns="http://wadl.dev.java.net/2009/02">

  <resources base="http://dev.java.net/">
    <resource path="projects/{id}">
      <method name="GET">
        <request>
          <param name="id" style="template">
            <doc>
              The identifier of the project.
            </doc>
          </param>
        </request>
        <response status="200">
          <representation mediaType="text"/>
        </response>
      </method>
      
    </resource>
  </resources>

</application>
